{
  "schemaVersion": 1,
  "id": "labscraft",
  "version": "${version}",
  "name": "LabsCraft",
  "description": "A satirical Google Labs RPG mod for Minecraft",
  "authors": [
    "LabsCraft Team"
  ],
  "contact": {
    "homepage": "https://github.com/yourusername/labscraft",
    "sources": "https://github.com/yourusername/labscraft"
  },
  "license": "MIT",
  "icon": "assets/labscraft/icon.png",
  "environment": "*",
  "entrypoints": {
